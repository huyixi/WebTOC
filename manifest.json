{
  "manifest_version": 3,
  "name": "Sidepanel MVP",
  "version": "1.0",
  "action": {
    "default_title":"Sidepanel MVP"
  },
  "permissions": ["activeTab", "storage", "sidePanel", "tabs"],
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "background":{
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
